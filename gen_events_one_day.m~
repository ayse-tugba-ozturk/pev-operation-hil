function events = gen_events_one_day()
% generates a sequence of events

num_events = 10;
events.inp = cell(num_events,1);

for n = 1:num_events
    event.time = k;
    event.SOC_init = 0.2;
    event.SOC_need = 0.4; % add infeasible scenario
    event.batt_cap = 24;
    event.duration = 3;
    event.overstay_duration = 2.3;
    event.pow_max = 3.2;
    event.pow_min = 0;
    events.inp{n} = event;
    events.time
end

end